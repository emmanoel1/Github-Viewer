{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "trybe-frontend"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "sonarjs"]
}
